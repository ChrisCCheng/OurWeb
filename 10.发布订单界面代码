<script>
	//Javascript表单检验
	function checkSubmit($o){
		var pass1 = $o.pass1.value;
		var pass2 = $o.pass2.value;
		var fail = "";
		if(pass1!=pass2){
			fail += '两次输入的密码不相同\n\r';
		}
		if(fail == ""){
			return true;
		}else{
			alert(fail);
			return false;
		}
	}

</script>
